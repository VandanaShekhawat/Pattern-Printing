public class MyClass {
    public static void main(String args[]) {
      int x=8790;
      int sum=0;
      int rem =0;
    while(x!=0)
    {            // To calculate sum of entered number by user
    rem = x%10;
       sum = sum+rem; 
      x = x/10;
    }
         System.out.println(sum);
        }
    }
