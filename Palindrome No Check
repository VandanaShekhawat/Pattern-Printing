public class MyClass {
    public static void main(String args[]) {
      int no = 535;
      int no1 = no;
    int rem = 0, rev=0;
    while(no!=0){
        rem = no%10;
        rev = rev*10+rem;
        no = no/10;
    }
    System.out.println(rev);
    if( rev == no1){
        System.out.println("No is palindrome ");
    }
    else {
      System.out.println("No is not  palindrome");
    
    }
}
}
