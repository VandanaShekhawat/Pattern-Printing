public class MyClass {
    public static void main(String args[]) {
    int a = 0; int b = 1;
    int c=0;
    int no = 10;
    System.out.println(a);          
                                                        // fibonacci Series 
    for(int i = 1;i<=no;i++){
        c = a+b;
        System.out.println(c);
        a=b;
        b=c;
        }
    }
}
