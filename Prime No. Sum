public class Prime {
    public static void main (String args [])
    {
      
        int sum = 0;
        int temp = 0;
        for(int j=1;j<=100;j++)
        {
        for(int i = 2;i<=j-1;i++)
        {
            if(j%i==0)
            {
                temp = temp +1;
            }
        }
        if(temp == 0){
          sum= sum +j;
        }
        
        else
        {
         temp = 0;
        }
        }
         System.out.println("Sum of prime no is ::"+ sum  );
        
    
    }
}
